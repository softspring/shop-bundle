{
    "name": "softspring/shop-bundle",
    "description": "A shop bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "softspring/catalog-bundle": "^1.0@dev",
        "softspring/crudl-bundle": "^1.0.1",
        "softspring/doctrine-templates": "^1.1@dev",
        "symfony/workflow": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "doctrine/common": "^2.12",
        "doctrine/orm": "^2.7",
        "symfony/http-foundation": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\ShopBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\ShopBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
